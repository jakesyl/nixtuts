---
---
[
  {% for page in site.posts %}
  {% if page.status != draft && page.title != null && page.category != null && page.tags != null && page.summary != null && page.url != null %}
  {
    "title":"{{page.title}}",
    "category":"{{page.category}}", 
    "tags":[{% for tag in page.tags %}"{{tag}}",{% endfor %} null],
    "summary":"{{ page.summary | replace:'"','''}}",
    "url":"{{ page.url }}",
    
    {% if page.date == null %}
    "date":"NA"
    {% else %}
    "date":"{{ page.date | date: "%d %B %Y"  }}"
    {% endif %}
  },
  {% endif %}
  {% endfor %}
  null
]

